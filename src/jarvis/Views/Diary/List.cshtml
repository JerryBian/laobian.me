@model List<DateTime>
@{
    Layout = "_DiaryLayout";
    ViewData["Title"] = "过滤的所有日记";
}

<ul>
    @foreach (var item in Model.OrderByDescending(x => x))
    {
        var url = $"/diary/{item.Year:D4}/{item.ToDate()}.html";
        <li>
            <a href="@url">@item.ToChinaDate()</a>
        </li>
    }
</ul>