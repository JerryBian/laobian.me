@model List<NoteRuntime>
@inject IOptions<JarvisOptions> _options
@{
    Layout = "_NoteLayout";
    ViewData["Title"] = "所有笔记";
}

<div>
    <ul>
        @foreach (var item in Model.OrderByDescending(x => x.Raw.CreateTime))
        {
            <li>
                <a href="@item.Raw.GetFullPath(_options.Value)">@item.Raw.Title</a>
            </li>
        }
    </ul>
</div>