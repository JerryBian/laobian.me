@inject IOptions<LaobianOption> _option
@{
    Layout = "_Layout";
    ViewData["BrandName"] = $"{_option.Value.Title}的博客";
    ViewData["BrandLink"] = "/blog";
}

<div>
    @RenderBody()
</div>

@section Script{
    <script>
        activeNavItem("#nav_blog");
    </script>
    @await RenderSectionAsync("Script", false)
}