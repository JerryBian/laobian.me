@inject ISwanService _service
@{
    Layout = "_LayoutAdmin";
}

<div>
    @foreach (var item in await _service.GetReadItemsAsync())
    {
        <div>
            <a href="@item.GetFullLink()">@item.BookName</a>
            <a href="/admin/read-edit/@item.Id">Edit</a>
        </div>
    }
</div>

@section Script{
    <script>
        setActive(document.querySelector("#navHeading_reads"));
    </script>
}